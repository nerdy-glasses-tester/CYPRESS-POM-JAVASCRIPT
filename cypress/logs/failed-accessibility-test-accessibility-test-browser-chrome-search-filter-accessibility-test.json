{
  "specName": "AccessibilityTest.spec.js",
  "title": "Browser: chrome - Search Filter Accessibility Test",
  "suiteName": "Accessibility test",
  "testName": "Accessibility test Browser: chrome - Search Filter Accessibility Test",
  "testError": "10 accessibility violations were detected: expected 10 to equal 0",
  "testCommands": [
    "visit https://www.apartments.com",
    "get #quickSearchLookup",
    "assert expected **<input#quickSearchLookup.quickSearchLookup>** to be **visible**",
    "get #quickSearchLookup",
    "type Irvine, CA, {animationdistancethreshold: 20}",
    "get .item>.matchText",
    "xhr  POST https://www.apartments.com/services/geography/search/",
    "xhr  POST https://p.tvpixel.com/com.snowplowanalytics.snowplow/tp2",
    "get .go",
    "click {animationdistancethreshold: 20}",
    "xhr  POST https://www.apartments.com/services/url/",
    "wait 5000",
    "page load --waiting for new page to load--",
    "new url https://www.apartments.com/irvine-ca/",
    "xhr  POST https://p.tvpixel.com/com.snowplowanalytics.snowplow/tp2",
    "xhr  GET https://shapes.apartments.com/shapes/city/25931/high/",
    "xhr  GET https://ct.pinterest.com/user/?tid=2612511761707&pd={\"np\":\"gtm\"}&cb=1621199968783",
    "xhr  POST https://ct.pinterest.com/md/",
    "get title",
    "assert expected **'<title>'** to have text **'&nbsp;Apartments for Rent in Irvine CA | Apartments.com'**",
    "wait 3000",
    "readFile ./node_modules/axe-core/axe.min.js",
    "a11y error! button-name on 56 Nodes",
    "a11y error! color-contrast on 3 Nodes",
    "a11y error! duplicate-id-active on 4 Nodes",
    "a11y error! duplicate-id-aria on 9 Nodes",
    "a11y error! duplicate-id on 27 Nodes",
    "a11y error! empty-heading on 1 Node",
    "a11y error! heading-order on 1 Node",
    "a11y error! image-alt on 451 Nodes",
    "a11y error! link-name on 25 Nodes",
    "a11y error! meta-viewport on 1 Node",
    "assert 10 accessibility violations were detected: expected **10** to equal **0**"
  ]
}
